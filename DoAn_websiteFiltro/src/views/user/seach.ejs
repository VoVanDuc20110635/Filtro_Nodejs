                                    <a class="dropdown-item" id="product_name_desc" onclick="submitHiddenFormFromSortType(this.id)">Product Name: Z-A</a>
                                    <a class="dropdown-item" id="price_asc" onclick="submitHiddenFormFromSortType(this.id)">Price Ascending</a>
                                    <a class="dropdown-item" id="price_desc" onclick="submitHiddenFormFromSortType(this.id)">Price Descending</a>
                                    <a class="dropdown-item" id="newest" onclick="submitHiddenFormFromSortType(this.id)">Newest</a>
                                    <a class="dropdown-item" id="oldest" onclick="submitHiddenFormFromSortType(this.id)">Oldest</a>
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='best_selling')}">Top-selling product</a>-->
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='product_name_asc')}">Product Name: A-Z</a>-->
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='product_name_desc')}">Product Name: Z-A</a>-->
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='price_asc')}">Price Ascending</a>-->
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='price_desc')}">Price Descending</a>-->
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='newest')}">Newest</a>-->
                                    <!--                                <a class="dropdown-item" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id},sortType='oldest')}">Oldest</a>-->
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row pb-3 px-xl-5" >

                        <div th:each="product : ${products}" class="col-lg-3-three col-md-6 col-sm-12 pb-1">
