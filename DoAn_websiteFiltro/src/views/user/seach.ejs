                    <h2 class="text-center">No matching products found!</h2>

                </div>
            </th:block>
            <th:block th:if="${not #lists.isEmpty(products)}">
                <!-- Shop Sidebar Start -->
                <div class="col-lg-3 col-md-12">
                    <form id="hiddenForm" th:href="@{/category/{id}(id=${currentIdAll == 'all' ? 'all' : category.id})}" method="GET" style="display: none;">
                        <input type="hidden" name="lowPrice" th:value="${dataLowPrice}" />
                        <input type="hidden" name="highPrice" th:value="${dataHighPrice}" />
                        <input type="hidden" name="sortType" th:value="${sortType}" />
                        <input type="hidden" name="materialId" th:value="${dataMaterialId}" />
                        <input type="hidden" name="currentPage" th:value="${currentPage}" />
                    </form>
                    <!-- Price Start -->
                    <div class="border-bottom mb-4 pb-4">
                        <h5 class="font-weight-semi-bold mb-4">Filter by price</h5>
                        <form>
                            <div class="custom-control custom-checkbox d-flex align-items-center justify-content-between mb-3">
                                <input type="checkbox" class="custom-control-input" id="price-all" value="0,1000" onchange="submitHiddenFormFromSortType(this.value)" th:checked="${dataLowPrice == 0 && dataHighPrice == 1000}">
                                <label class="custom-control-label" for="price-all">All Price</label>

                            </div>
                            <div class="custom-control custom-checkbox d-flex align-items-center justify-content-between mb-3">
                                <input type="checkbox" class="custom-control-input" id="price-1" value="0,50" onchange="submitHiddenFormFromSortType(this.value)" th:checked="${dataLowPrice == 0 && dataHighPrice == 50}">
                                <label class="custom-control-label" for="price-1">$0 - $50</label>

                            </div>
                            <div class="custom-control custom-checkbox d-flex align-items-center justify-content-between mb-3">
                                <input type="checkbox" class="custom-control-input" id="price-2" value="50,100" onchange="submitHiddenFormFromSortType(this.value)" th:checked="${dataLowPrice == 50 && dataHighPrice == 100}">
                                <label class="custom-control-label" for="price-2">$50 - $100</label>

                            </div>
                            <div class="custom-control custom-checkbox d-flex align-items-center justify-content-between mb-3">
                                <input type="checkbox" class="custom-control-input" id="price-3" value="100,150" onchange="submitHiddenFormFromSortType(this.value)" th:checked="${dataLowPrice == 100 && dataHighPrice == 150}">
                                <label class="custom-control-label" for="price-3">$100 - $150</label>

                            </div>
                            <div class="custom-control custom-checkbox d-flex align-items-center justify-content-between mb-3">
                                <input type="checkbox" class="custom-control-input" id="price-4" value="150,200" onchange="submitHiddenFormFromSortType(this.value)" th:checked="${dataLowPrice == 150 && dataHighPrice == 200}">
                                <label class="custom-control-label" for="price-4">$150 - $200</label>

                            </div>
                            <div class="custom-control custom-checkbox d-flex align-items-center justify-content-between">
                                <input type="checkbox" class="custom-control-input" id="price-5" value="200,500" onchange="submitHiddenFormFromSortType(this.value)" th:checked="${dataLowPrice == 200 && dataHighPrice == 1000}">
                                <label class="custom-control-label" for="price-5">Over $200</label>

                            </div>
                        </form>
                    </div>
                    <!-- Price End -->

                    <!-- Color Start -->
                    <div class="border-bottom mb-4 pb-4">
